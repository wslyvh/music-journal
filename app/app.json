{
  "expo": {
    "name": "Music Journal",
    "description": "Transform your music practice with Music Journal, the app that helps you record, reflect, and improve.",
    "slug": "music-journal-app",
    "version": "0.1.0",
    "githubUrl": "https://github.com/wslyvh/music-journal",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon.png",
    "scheme": "music-journal",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#14181c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#fc766a"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
